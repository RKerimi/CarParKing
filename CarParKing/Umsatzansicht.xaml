<Page x:Class="CarParKing.Umsatzansicht"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      x:Name="salesPage"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CarParKing"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Umsatzansicht">

    <Grid >
        <Menu Grid.Row="0" Grid.ColumnSpan="2" IsMainMenu="True" Margin="0,0,0.4,12.4">
            <MenuItem Click="ticketClick" Header="_View" />
            <MenuItem Click="umsatzClick" Header="_Umsatz" />
            <MenuItem Click="mainClick" Header="Main" />
            <MenuItem Header="_Help" />
        </Menu>

        <TextBlock Text="Umsatzansicht" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24"/>
    </Grid>
</Page>
